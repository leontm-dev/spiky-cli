{
	"display": "Node LTS",
	"_version": "20.1.0",
	"compilerOptions": {
		"lib": ["es2023"],
		"module": "node16",
		"target": "es2022",
		"strict": true,
		"noUncheckedIndexedAccess": true,
		"outDir": "dist",
		"esModuleInterop": true,
		"skipLibCheck": true,
		"moduleResolution": "node16"
	},
	"include": ["src/**/*.ts"]
}
